<%- include('../includes/head.ejs')%>
    <!-- <link rel="stylesheet" href="/css/form.css"> -->
    </head>

    <body>
        <%- include('../includes/nav.ejs')%>
            <div class="mainbox">
                <% for ( let data of list){%>
                    <div class="mainbox_box1 card1">


                        <button type="submit">
                            <lable>Name</lable>: <%= data.name%> <br>
                                <lable>ADhar no</lable>: <%= data.adharno%> <br>
                                    <lable>Document</lable>: <%= data.documenttype%> <br><br>
                        </button>
                        <form action="/admin/deletedocument" method="post">
                            <button type="submit" class="btn btn-primary"
                                style="background-color: blue;margin: 10px;padding: 30px;"><i class="fa-solid fa-trash"></i></button>
                            <input type="hidden" name="_csrf" id="" value="<%= csrfToken %>">
                            <input type="hidden" value="<%=data._id %>" name="productId">
                        </form>
                        <form action="/admin/requestdocument" method="post">
                            <input type="hidden" value="<%=data.adharno %>" name="adharno">
                            <input type="hidden" value="<%=data._id %>" name="productId">
                            <input type="hidden" name="_csrf" id="" value="<%= csrfToken %>">
                            <button type="submit" class="btn btn-primary"
                                style="background-color: blue;margin: 10px; padding: 30px;">View Detail</button>
                        </form>
                    </div>


                    <% }%>
            </div>


            <%- include('../includes/end.ejs')%>